{
  "name": "frameworks/slim",
  "description": "Slim Skeleton",
  "minimum-stability": "dev",
  "license": "proprietary",
  "version": "0.0.1",
  "authors": [
    {
      "name": "William Stam",
      "email": "william@munsoft.co.za"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0"
    }
  },
  "require": {
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "illuminate/database": "8.x-dev",
    "intervention/image": "^2",
    "laminas/laminas-escaper": "^2",
    "php-di/php-di": "^6",
    "psr/log": "^1",
    "slim/php-view": "^3",
    "slim/psr7": "dev-master",
    "slim/slim": "^4",
    "zeuxisoo/slim-whoops": "^0.7",
    "zircote/swagger-php": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "System\\": "system/",
      "Modules\\": "modules/",
      "App\\": "app/"
    }
  }
}
